  "browserslist": [
    "last 4 version"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.3",
    "postcss": "^8.4.35",
    "postcss-assets": "^6.0.0",
